{
    "manifest_version": 3,
    "name": "Skip Add",
    "author": "Mustak24",
    "description": "Skip YT ads",
    "version": "1.0",
    "permissions": ["activeTab"],
    "content_scripts": [
      {
        "matches": ["https://www.youtube.com/*"],
        "js": ["script.js"]
      }
    ],
    "action": {
      "default_popup": "index.html",
      "default_icon": {
        "128": "logo.png"
      }
    }
  }
  